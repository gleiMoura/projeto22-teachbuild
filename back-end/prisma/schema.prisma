generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model teachers {
  id           Int         @id @unique @default(autoincrement())
  name         String
  email        String @unique
  password     String
  image        String
  text         String
  likeId       Int 
  mbtiId       Int 
  disciplineId Int
  disciplines  disciplines @relation(fields: [disciplineId], references: [id])
  likes        likes       @relation(fields: [likeId], references: [id])
  mbti         mbti        @relation(fields: [mbtiId], references: [id])
}

model likes {
  id        Int        @id @unique @default(autoincrement())
  teacherId Int
  numbers   Int
  teachers  teachers[]
}

model mbti {
  id       Int        @id @unique @default(autoincrement())
  name     String
  teachers teachers[]
}

model disciplines {
  id       Int        @id @unique @default(autoincrement())
  name     String
  teachers teachers[]
}
